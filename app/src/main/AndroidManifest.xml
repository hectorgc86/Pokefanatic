<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hector.proyectofinalbloque2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:fullBackupContent="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/nombre_app"
        android:roundIcon="@drawable/pokeball"
        android:supportsRtl="true"
        android:theme="@style/tema_app">
        <activity
            android:name=".Detalles"
            android:label="@string/title_activity_detalles"
            android:parentActivityName=".Principal"
            />
        <activity
            android:name=".FotosInternet"
            android:label="@string/title_activity_fotos_internet"
            android:parentActivityName=".Detalles"
            />
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>